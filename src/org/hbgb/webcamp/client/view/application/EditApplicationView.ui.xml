<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	         xmlns:g="urn:import:com.google.gwt.user.client.ui"
	         xmlns:w="urn:import:org.hbgb.webcamp.client.widget">

	<g:CaptionPanel captionText="Application View">
		<g:VerticalPanel>
		
			<g:HorizontalPanel>
				
				<g:Button ui:field="prevButton">Prev</g:Button>
				<g:Button ui:field="listButton">List</g:Button>
				<g:Button ui:field="nextButton">Next</g:Button>
				
			</g:HorizontalPanel>
			
			<g:ScrollPanel>
			
				<g:VerticalPanel ui:field="content">
					
				</g:VerticalPanel>
				
			</g:ScrollPanel>
			
		</g:VerticalPanel>
	</g:CaptionPanel>
</ui:UiBinder> 