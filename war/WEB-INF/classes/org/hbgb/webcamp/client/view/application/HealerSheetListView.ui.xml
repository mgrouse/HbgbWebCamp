<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		
		.applicationViewFlexTable 
		{
			margin: 5px 0px 5px 0px;
		}
	</ui:style>

	<g:ScrollPanel>
		<g:VerticalPanel>
	
			<g:CaptionPanel captionText="List of Healer Sheets">
				<g:VerticalPanel>
				
					<g:FlexTable ui:field="table" addStyleNames="{style.applicationViewFlexTable}"/>
				
				</g:VerticalPanel>
			</g:CaptionPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>
			
</ui:UiBinder> 